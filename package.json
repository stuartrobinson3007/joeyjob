{
  "name": "joeyjob",
  "private": true,
  "type": "module",
  "engines": {
    "node": "20"
  },
  "scripts": {
    "dev": "vite dev --port 5722",
    "start": "node .output/server/index.mjs",
    "build": "vite build",
    "serve": "vite preview",
    "test": "vitest run",
    "taali:push": "git subtree push --prefix=src/taali taali main",
    "taali:pull": "git subtree pull --prefix=src/taali taali main --squash",
    "taali:status": "git log --oneline --grep='git-subtree-dir: src/taali' | head -3",
    "taali:check": "echo 'Checking taali structure...' && ls -la src/taali/",
    "taali:remote": "git remote get-url taali",
    "seed:todos": "tsx scripts/seed-todos.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/ --ext .ts,.tsx",
    "lint:fix": "eslint src/ --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "i18n:extract": "i18next-parser",
    "i18n:extract:watch": "i18next-parser --watch",
    "i18n:audit": "node scripts/find-missing-translations.cjs",
    "i18n:check-keys": "node scripts/find-missing-keys.cjs",
    "i18n:check-all": "npm run i18n:check-keys && npm run i18n:audit",
    "i18n:missing": "i18next-parser --fail-on-warnings",
    "i18n:validate": "i18next-parser --silent",
    "errors:check": "node scripts/find-unused-errors.cjs",
    "errors:check:strict": "node scripts/find-unused-errors.cjs --strict",
    "errors:fix": "node scripts/find-unused-errors.cjs --fix",
    "errors:check:all": "npm run errors:check && npm run lint -- --quiet",
    "errors:check:full": "npm run errors:check && npm run lint",
    "email:dev": "email dev --dir src/emails",
    "db:migrate": "drizzle-kit migrate",
    "db:generate": "drizzle-kit generate",
    "db:reset": "drizzle-kit reset",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "docker:dev": "docker compose -f docker-compose.yml up -d --build",
    "docker:down": "docker compose -f docker-compose.yml down",
    "docker:reset": "docker compose -f docker-compose.yml down -v && docker compose -f docker-compose.yml up -d --build"
  },
